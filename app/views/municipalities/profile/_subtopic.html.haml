%thead.subtopic{id: "#{subtopic.slug}"}
  %th.attr #{subtopic}
  %th.muni #{@profile.muni}
  %th.operation
  %th
    %span.defined
      %a{data: {toggle: 'modal', target: '#neighbors'}} #{@profile.neighbors}
    %span.geography
      Municipalities
  %th
    %span.defined
      %a{data: {toggle: 'modal', target: '#community-type'}} #{@profile.community_type}
    %span.geography Community Type
  %th
    %span.defined   #{@profile.region}
    %span.geography Region
  %th
    %span.defined   #{@profile.county}
    %span.geography County
  %th
    %span.defined   #{@profile.state.abbr}
    %span.geography State
= render partial: 'municipalities/profile/field', collection: subtopic.fields